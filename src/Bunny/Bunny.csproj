<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Bunny</AssemblyName>
    <TargetFramework>net7.0</TargetFramework>
    <Version>0.3.0</Version>
    <Nullable>enable</Nullable>
    <LangVersion>12</LangVersion>

    <Authors>iamcarbon</Authors>
    <Description>A modern API for Bunny.net.</Description>
    <PackageId>Bunny</PackageId>
    <PackageTags>bunny.net DNS CDN</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/carbon/Bunny</PackageProjectUrl>
  </PropertyGroup>
</Project>